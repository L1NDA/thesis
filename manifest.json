{
  "name": "Thesis",
  "version": "1.0",
  "description": "Stores variables more easily",
  "icons": {
    "128": "./images/icon_128.png",
    "128": "./images/icon_128.png",
    "128": "./images/icon_128.png",
    "128": "./images/icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["scripts/background_script.js"]
  },
  "content_scripts": [
   {
     "matches": ["<all_urls>", "file://*/*", "*://*.pdf/"],
     "js": ["scripts/content_script.js", "scripts/findAndReplaceDOMText.js"],
     "run_at": "document_start",
     "all_frames": true
   }
 ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Activate thesis walrus!"
  },
  "permissions": ["file://*/*", "activeTab", "tabs", "http://*/*", "https://*/*", "storage", "<all_urls>"],
  "browser_action": {
    "default_icon": "./images/icon_128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
